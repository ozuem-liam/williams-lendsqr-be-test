{
  "name": "williams-lendsqr-be-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run nodemon",
    "start:start": "npm run build && node build/index.js",
    "start": "set APP_ENV=production; node -r ts-node/register/transpile-only -r tsconfig-paths/register -r dotenv/config build/index.js",
    "build": "tsc -p ./tsconfig.json",
    "start:prod": "npm run build && npm run start",
    "nodemon": "nodemon --watch \"src/**/*.ts\" --ignore \"src/**/*.spec.ts\" -r ts-node/register/transpile-only -r tsconfig-paths/register -r dotenv/config --exec \"ts-node --files\" src/index.ts",
    "db:migrate:latest": "knex --knexfile src/database/knexfile.ts migrate:latest",
    "db:migrate:up": "knex --knexfile src/database/knexfile.ts migrate:up",
    "db:migrate:rollback": "knex --knexfile src/database/knexfile.ts migrate:rollback",
    "test": "jest -i --detectOpenHandles --passWithNoTests",
    "test:watch": "jest --watchAll -i --detectOpenHandles",
    "coverage": "jest --coverage -i --detectOpenHandles",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "test:e2e": "jest -i -c jest.integration.config.js",
    "test:e2e:watch": "jest -i -c jest.integration.config.js --watchAll",
    "test:e2e:coverage": "jest -i -c jest.integration.config.js --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "type:check": "tsc --project tsconfig.json --noEmit",
    "prepare": "husky install"
  },
  "author": "Williams",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/easy-soap-request": "^4.1.1",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.5",
    "@types/glob": "^7.1.3",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^26.0.23",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/nodemailer": "^6.4.6",
    "@types/supertest": "^2.0.11",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "coveralls": "^3.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.27.0",
    "faker": "5.5.3",
    "husky": "6",
    "jest": "26.6.3",
    "prettier": "^2.3.0",
    "supertest": "^6.1.3",
    "ts-jest": "26.5.6",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@joi/date": "^2.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^3.3.0",
    "agenda": "^4.2.1",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "config": "^3.3.8",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dayjs": "^1.10.7",
    "easy-soap-request": "^4.2.0",
    "express": "^4.17.1",
    "fast-xml-parser": "^3.20.3",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "http-status": "^1.5.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.5.1",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "nanoid": "^3.1.29",
    "nodemailer": "^6.8.0",
    "nodemon": "^2.0.7",
    "pino": "^7.6.2",
    "slugify": "^1.6.0",
    "sqlite3": "^5.1.6",
    "xml2js": "^0.4.23",
    "xss-clean": "^0.1.1"
  }
}
